{
  "name": "dangerjs-wrapper",
  "version": "1.0.5",
  "description": "Apply cultural rules during your CI process",
  "bin": "command.js",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": "https://github.com/guidesmiths/dangerjs-wrapper",
  "author": "guidesmiths",
  "license": "ISC",
  "keywords": [
    "danger"
  ],
  "dependencies": {
    "danger": "^10.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
